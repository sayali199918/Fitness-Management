		http_request.send("para="+parameters);
		}
}
function fnPopulateEvaluationDiv(http_request,fieldName)	
{	
if (http_request.readyState == 4) 
	{
		if (http_request.status == 200) 
		{
			if (http_request.responseText != null) 
			{
			var newTable = jQuery('<table id="evaluationdownloadList" width="100%" border="0" cellspacing="0" cellpadding="0" class="tablelist"></table><br></br><br></br>');
			var newRow = jQuery('<tr></tr>');
			var newTd = jQuery('<td colspan="3" class="sectionheading">Download List</td>');
			newRow.append(newTd);
			newTable.append(newRow);
			var newRow = jQuery('<tr id="tr_0"></tr>');
			var newTd = jQuery('<td id="td_0" class="tdheader" width="60%">Subject</td><td id="td_1" class="tdheader">Question Paper</td><td id="td_2" class="tdheader">AnswerSheet</td>');
			newRow.append(newTd);
			newTable.append(newRow);
			/*var responseDoc= http_request.responseXML;
				var docs = responseDoc.getElementsByTagName("Row");*/
				var responseDoc = "";
				var docs = "";
				if(http_request.responseXML!=null){
					responseDoc = http_request.responseXML;
					docs = responseDoc.getElementsByTagName("Row");
				}else{
					responseDoc = http_request.responseText;
					docs = $(responseDoc).find("Row");
				}
	       		for(var i=0; i<docs.length;i++)
	       		{
	       			var id=i+1;
	       			var subjectName='';
	       			var qpFunction='';
	       			var answersheetLink='';
	       			 var qpfunctionArray='';
	       			var docFields=docs[i].getElementsByTagName("Field");
	       			if(docFields[0].firstChild!=null){
	       				subjectName=docFields[0].firstChild.nodeValue;
	       			}
	       			if(docFields[1].firstChild!=null){
	       				qpfunctionArray=docFields[1].firstChild.nodeValue.split(",");
	       				qpFunction='fndownloadEvaluationQp("'+qpfunctionArray[0]+'","'+qpfunctionArray[1]+'","'+qpfunctionArray[2]+'",'+qpfunctionArray[3]+','+qpfunctionArray[4]+')';
	       			}
	       			else
	       			{
	       				qpFunction="N.A";
	       			}
	       			if(docFields[2].firstChild!=null)
	       			{
	       				answersheetLink=docFields[2].firstChild.nodeValue;
	       			}
	       			else
	       			{
	       			answersheetLink="N.A";
	       			}
	       			var newRow = jQuery('<tr id="tr_'+id+'"></tr>');
					var newTd = "<td id='td_0' >"+subjectName+"</td><td id='td_1'><a onclick='javascript:"+qpFunction+";' style='cursor pointer;cursor and;'><img src='images/download_icon.jpg' title = 'Download Question Paper'/></a></td><td id='td_2'><a onclick=\"javascript:downloadAnswerSheet('"+answersheetLink+"');\" style='cursor pointer;cursor and;'><img src='images/download_icon.jpg'/></a></td>";
					newRow.append(newTd);
					newTable.append(newRow);
					jQuery('#evalQpAndAnswerSheetDiv').append(newTable)

					}
			}
		}
	}
}
function fndownloadEvaluationQp(appSeqNo,answersheetCode,driveId,subjectId,assessmentId)
{

	var orgId = document.getElementById('orgId').value;
	var formId = document.getElementById('formId').value;
	var checkSum = document.getElementById('checksum').value;
	var qpTabNo=document.getElementById('qpPblishTabId').value;
	var tabId=parseInt(qpTabNo.split("-")[1])-28+1;
	
	
	var newWin = window.open("/ASM/jsp/HallTicket.jsp", "newWin",
			"height=500,width=700,resizable=yes,scrollbars=yes");
	newWin.document
			.write("<html><body><form id='qpform' name='qpform' action='/ASM/QPDownloadServlet' method ='post'><input type='hidden' id='qpappSeqNo' name='qpappSeqNo' value='"
					+ appSeqNo
					+ "'><input type='hidden' id='qpsubject' name='qpsubject' value='"
					+ subjectId
					+ "'><input type='hidden' id='qpdriveId' name='qpdriveId' value='"
					+ driveId
					+ "'><input type='hidden' id='qpassessmentId' name='qpassessmentId' value='"
					+ assessmentId
					+ "'><input type='hidden' id='qporgId' name='qporgId' value='"
					+ orgId
					+ "'><input type='hidden' id='qpcheckSum' name='qpcheckSum' value='"
					+ checkSum
					+ "'><input type='hidden' id='qpformId' name='qpformId' value='"
					+ formId + "'><input type='hidden' id='qpanswerSheetCode' name='qpanswerSheetCode' value='"
					+ answersheetCode + "'></form></body></html>");
	newWin.document.getElementById('qpform').submit();
	setTimeout("showTab("+tabId+")", 2000);
}
	
