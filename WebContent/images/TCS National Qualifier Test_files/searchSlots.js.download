function fnPopulateDataNew(http_request,htmlfieldName)
{
var length=document.getElementById(htmlfieldName).options.length;
for(var i=0; i<length; i++)
{
document.getElementById(htmlfieldName).remove(1);
}
document.getElementById(htmlfieldName).style.display='';
	if (http_request.readyState == 4) {
		if (http_request.status == 200) {
			if(http_request.responseText != null) {
				var responseDoc = "";
				var docs = "";
				if(http_request.responseXML!=null){
					responseDoc = http_request.responseXML;
					docs = responseDoc.getElementsByTagName("Row");
				}else{
					responseDoc = http_request.responseText;
					docs = $(responseDoc).find("Row");
				}
	       		for(var i=0; i<docs.length;i++)
	       		{
	       			var docFields=docs[i].getElementsByTagName("Field");
	       			if(docFields[0].firstChild!=null){
	       				var value=docFields[0].getAttribute("fieldname");
	       				var text=docFields[0].firstChild.nodeValue;
	       				document.getElementById(htmlfieldName).options[parseInt(i)+1] = new Option(text, value);
	       			}
			    }
			}
		}
	}
}
