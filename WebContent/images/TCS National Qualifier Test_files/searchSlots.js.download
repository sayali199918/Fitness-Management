function createEvaluationDiv()
{
	$(document.getElementById('evaluationdownloadList')).remove();
	var http_request=fnGetXMLHttpObject();
	var assessmenId=document.getElementById('txtEvaluation').value;
	if(assessmenId=='' || assessmenId==0)
	{
	alert("Please selecet an evaluation");
	return false;
	}
	if(http_request!=null)
		{
		var appSeqNo=document.getElementById("app_seq_no").value;
		var formId=document.getElementById("formId").value;
		var orgId=document.getElementById("orgId").value;
		var parameters = "getEvaluationDownloadList@@"+appSeqNo+"@@"+formId+"@@"+orgId+"@@"+assessmenId;
		var pValidationServlet = "/ASM/CandidateDataValidationServlet"; 
		http_request.open('POST',pValidationServlet,false);       
		http_request.onreadystatechange = function() {}; 	 	                    
		http_request.onreadystatechange = function() { fnPopulateEvaluationDiv(http_request,'txtEvaluation'); }; 	 	                    
