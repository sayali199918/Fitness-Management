function fnPopulateHTData(http_request, assessmentId,tabId){
	if (http_request.readyState == 4) {
		if (http_request.status == 200) {
			if (http_request.responseText != null) {
				if(document.getElementById('htDiv')!=null && document.getElementById('htDiv')!=undefined){
					jQuery('#htDiv').html('');
				}
				var responseDoc = "";
				var docs = "";
				var rows;
				if(http_request.responseXML!=null){
					responseDoc = http_request.responseXML;
					docs = responseDoc.getElementsByTagName("RESPONSE");
					rows=docs[0].getElementsByTagName("ROW");
				}else{
					responseDoc = http_request.responseText;
					docs = $(responseDoc).find("Row");
					rows=docs;
				}
				var htType=$(responseDoc).attr("HTTYPE");
				//var rows = docs[0].getElementsByTagName("ROW");
				var div=jQuery('<div id="htDiv"></div>');
				var table=jQuery('<table id="htTable" class="tablelist" width="100%" border="0" cellspacing="0" cellpadding="0"></table>');
				var header='';
				var newRow='';
				
			
				
				if(htType=='Assessment'){
					header=jQuery('<tr height="25px;"><th>Assessment</th><th>Action</th></tr>');
				}
				else if(htType=='Course'){
					header=jQuery('<tr height="25px;"><th>Assessment</th><th>Course</th><th>Action</th></tr>');
				}
				else if(htType=='Subject'){
					header=jQuery('<tr height="25px;"><th>Assessment</th><th>Course</th><th>Subject</th><th>Action</th></tr>');
				}
				else {
					header=jQuery('<tr height="25px;"><th>Assessment</th><th>Course</th><th>Subject</th><th>Action</th></tr>');
				}
				
				
				
				for(var i=0;i<rows.length;i++)
