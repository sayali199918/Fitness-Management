function fnBookSeat(bookedTestCenterName, boookedStartDate, bookedEndDate,
		bookedTcId, assessmentId) {
	var orgID = document.getElementById("orgId").value;
	var formID = document.getElementById("formId").value;
	// alert("bookedTcId:"+bookedTcId+" orgID:"+orgID+" formID:"+formID+"
	// bookedSlotId:"+bookedSlotId);
	var appSeqNo = "";
	if(document.getElementById('app_seq_no')!=null){
		appSeqNo = document.getElementById('app_seq_no').value;
	}else{
		appSeqNo = document.getElementById("userID").value;
	}
	// alert("appSeqNo:"+appSeqNo+" boookedStartDate:"+boookedStartDate+"
	// bookedEndDate:"+bookedEndDate);
	// alert("Selected Date:"+document.getElementById("txtChosenDate").value);
	var testFinalDate = document.getElementById("hdnTestDate").value;
	var testSubject = document.getElementById("hdnSubject").value;
	if (confirm("Are you sure you want to book this slot for " + testFinalDate
			+ "?")) {
		var http_request = fnGetXMLHttpObject();
		var parameters = orgID + "," + formID + "," + bookedTcId + ","
				+ boookedStartDate + "," + bookedEndDate + "," + appSeqNo + ","
				+ testFinalDate + "," + assessmentId + "," + testSubject;
		if (http_request != null) {
			var pValidationServlet = "/ASM/BookSlotServlet";
			http_request.open('POST', pValidationServlet, true);
			http_request.onreadystatechange = function() {
				fnAlertSlot(http_request, bookedTestCenterName, testFinalDate,
						boookedStartDate, bookedEndDate,appSeqNo);
			};
			http_request.setRequestHeader("Content-Type",
					"application/x-www-form-urlencoded");
			http_request.send("OrgFormParams=" + parameters);
		}
	} else {
	}
}
function fnAlertSlot(http_request, bookedTestCenterName, testFinalDate,
		boookedStartDate, bookedEndDate,appSeqNo) {
	if (http_request.readyState == 4) {
		if (http_request.status == 200) {
			if (http_request.responseText != null) {
				var responseDoc = http_request.responseText;
				if (responseDoc == 'success') {
					alert("Slot booked successfully for "
							+ bookedTestCenterName + " on " + testFinalDate
							+ " from " + boookedStartDate + " to "
							+ bookedEndDate);
					document.getElementById('userid').value = appSeqNo;
					document.loginForm.method="POST";
					document.loginForm.action="/EForms/loginAction.do?subAction=DisplayProfile";
					document.loginForm.submit();
				} else if (responseDoc == 'failure') {
					alert("Could not book slot. Please try again.")
				} else {
					alert(responseDoc);
				}
			}
		}
	}
}
