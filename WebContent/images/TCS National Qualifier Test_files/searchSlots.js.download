function fnPopulateTestSlotData(http_request, assessmentId,tabId) {
	if (http_request.readyState == 4) {
		if (http_request.status == 200) {
			if (http_request.responseText != null) {
				$('#bookingMessage').remove();
				var table = document.getElementById("testslots_tbody");
				for ( var i = table.rows.length - 1; i > 0; i--) {
					table.deleteRow(i);
				}
				document.getElementById("showedResults").style.display = "none";
				document.getElementById("totalRecords").style.display = "none";
				document.getElementById("pageNavPosition").style.display = "none";
				// document.getElementById("chosenDate").style.display="block";
				document.getElementById("ctl00_PlaceHolderMain_testslots").style.display = "block";
				/*var responseDoc = http_request.responseXML;
				// alert("responseDoc: "+responseDoc);
				var prefDoc = responseDoc.getElementsByTagName("_PREF");*/
				var responseDoc = "";
				var prefDoc = "";
				var responseString = "";
				if(http_request.responseXML!=null){
					responseDoc = http_request.responseXML;
					prefDoc = responseDoc.getElementsByTagName("_PREF");
				}else{
					responseDoc = http_request.responseText;
					responseDoc =responseDoc.replace(/_PREF/gi,"PREF");
					prefDoc = $(responseDoc).find("PREF");
				}
				if(prefDoc!=undefined && prefDoc.length>0){
					if (prefDoc[0].firstChild != null) {
						var pref = prefDoc[0].firstChild.nodeValue;
						if (pref == 'Yes') {
							if (document.getElementById('locationDetails') != null)
								document.getElementById('locationDetails').style.display='none';
						} else {
							if (document.getElementById('locationDetails') != null) {
								document.getElementById('locationDetails').style.display='';
							}
						}
					}
				}
				/*var docs = responseDoc.getElementsByTagName("TESTCENTER");
				var noOfDocuments = docs.length;*/
				var docsForBookedStatus= "";
				if(http_request.responseXML!=null){
					docsForBookedStatus = responseDoc.getElementsByTagName("BOOKEDTC");
				}else{
					docsForBookedStatus = $(responseDoc).find("BOOKEDTC");
				}
				
				if(docsForBookedStatus!=undefined && docsForBookedStatus.length>0){
					var tcName=docsForBookedStatus[0].getAttribute("TC_NAME");
					var cityName=docsForBookedStatus[0].getAttribute("CITY_NAME");
					var stateName=docsForBookedStatus[0].getAttribute("STATE_NAME");
					var countryName=docsForBookedStatus[0].getAttribute("COUNTRY_NAME");
					var examDate=docsForBookedStatus[0].getAttribute("EXAM_DATE");
					var startTime=docsForBookedStatus[0].getAttribute("START_TIME");
					var endTime=docsForBookedStatus[0].getAttribute("END_TIME");
					var subjectName=docsForBookedStatus[0].getAttribute("SUBJECT_NAME");
					var slotBookingLevel=docsForBookedStatus[0].getAttribute("BOOKING_LEVEL");
					var assessmentName=docsForBookedStatus[0].getAttribute("ASSESSMENT_NAME");
					
					responseString="You have booked your slot from "+startTime+" to "+endTime+" on "+examDate;
					if(slotBookingLevel!="Test Center Level"){
						responseString+=" at "+cityName+" for "+subjectName+" subject."
					}else{
						responseString+=" at "+tcName+", "+cityName+" for "+subjectName+" subject."
					}
				}
				
				var docs= "";
				if(http_request.responseXML!=null){
					docs = responseDoc.getElementsByTagName("TESTCENTER");
				}else{
					docs = $(responseDoc).find("TESTCENTER");
				}
				var noOfDocuments = docs.length;
				if (noOfDocuments <= 0) {
					if (document.getElementById('locationDetails') != null) {
						document.getElementById('locationDetails').style.display='';
					}
					if(responseString!=null && responseString!=""){
						document.getElementById('tcDataView').innerHTML='<div class="bookingMessage" id="bookingMessage" style="font-size: 13px;font-weight: normal;background-color: #FFFFCB;font-size: 11px;padding: 5px;font-weight: bold;color: #000000;position: relative;border: 1px solid #B9CAD4;">'
							+responseString+'</div>'+document.getElementById('tcDataView').innerHTML;
					}
					document.getElementById('testslots_tbody').innerHTML = document
							.getElementById('testslots_tbody').innerHTML
							+ "<tr id=\"noRecords\"><td colspan='3' id='noSlots'><b>No Test Centers Available</b></td></tr>";
				} else {
					if(responseString!=null && responseString!=""){
						document.getElementById('tcDataView').innerHTML='<div class="bookingMessage" id="bookingMessage" style="font-size: 13px;font-weight: normal;background-color: #FFFFCB;font-size: 11px;padding: 5px;font-weight: bold;color: #000000;position: relative;border: 1px solid #B9CAD4;">'
							+responseString+'</div>'+document.getElementById('tcDataView').innerHTML;
					}
					
					for (count = 0; count < noOfDocuments; count++) {
						var row = document.getElementById('testslots_tbody')
								.insertRow(count + 1);
						var cell1 = row.insertCell(0);
						var cell2 = row.insertCell(1);
						var cell3 = row.insertCell(2);
						cell1.style.width = '55%';
						cell2.style.width = '30%';
						cell2.style.align = 'center';
						
						cell3.style.width = '25%';
						var tcId = docs[count].getAttribute("TCID");
						var tcName = docs[count].getAttribute("TCNAME");
						tcName = tcName.replace(/\'/g, '');
						var tcAddress = docs[count].getAttribute("TCADDRESS");
						var tcCountry = docs[count].getAttribute("TCCOUNTRY");
						var tcState = docs[count].getAttribute("TCSTATE");
						var tcDistrict = docs[count].getAttribute("TCDISTRICT");
						var tcRefName = document.getElementById("testCenterReferral").value;
					
						cell1.innerHTML = "<table width='100%'><tr><td id='tcIdForHallTicket'><b>"+ tcName+ "</b><br/>"+ tcAddress+ ", "+ tcState + ", " + tcCountry + "</td><td>" +"</td></tr></table>";
						// }
						var docsSlot = docs[count].getElementsByTagName("SLOT");
						for (slotCount = 0; slotCount < docsSlot.length; slotCount++) {
							var docsSlotStartTime = docsSlot[slotCount].getElementsByTagName("STARTTIME");
							for (slotStartCount = 0; slotStartCount < docsSlotStartTime.length; slotStartCount++) {
								var startTimeValue = docsSlotStartTime[slotStartCount].firstChild.nodeValue;
							}
							var docsSlotEndTime = docsSlot[slotCount].getElementsByTagName("ENDTIME");
							for (slotEndCount = 0; slotEndCount < docsSlotEndTime.length; slotEndCount++) {
								var endTimeValue = docsSlotEndTime[slotEndCount].firstChild.nodeValue;
							}
					
							cell2.innerHTML = "<table align='center'><tr><td width='28%'></td><td style='text-align:center'>"+ startTimeValue + " to " + endTimeValue+ "</td><td width='15%'></td></tr></table>";
							var docsAvailableSeats = docsSlot[slotCount].getElementsByTagName("AVAILABLESEATS");
							for (slotAvailableSeatsCount = 0; slotAvailableSeatsCount < docsAvailableSeats.length; slotAvailableSeatsCount++) {
								var AvailableSeatValue = docsAvailableSeats[slotAvailableSeatsCount].firstChild.nodeValue;
							}
							var docsSlotID = docsSlot[slotCount].getElementsByTagName("SLOTID");
							for (slotIDCount = 0; slotIDCount < docsSlotID.length; slotIDCount++) {
								if(docsSlotID[slotIDCount].firstChild!=null && docsSlotID[slotIDCount].firstChild.nodeValue!=null){
									var SlotIdValue = docsSlotID[slotIDCount].firstChild.nodeValue;
								}
							}
							
							cell3.innerHTML = '<table width="100%"><tr><td>'
									+ AvailableSeatValue
									+ ' Seats </td><td><div align="right" class="detailsbutt1"><img src="images/onlineAppForm/book.gif" style="cursor: pointer;cursor:hand;" onclick="fnBookSeat(\''
									+ tcName + '\',\'' + startTimeValue
									+ '\',\'' + endTimeValue + '\',\'' + tcId
									+ '\',\'' + assessmentId
									+ '\');" /></div></td></tr></table>';
						}
					}
					var pager = new Pager('ctl00_PlaceHolderMain_testslots', 10);
					pager.init();
					pager.showPageNav('pager', 'pageNavPosition');
					pager.showPage(1);
					document.getElementById("showedResults").style.display = "";
					document.getElementById("totalRecords").style.display = "";
					document.getElementById("pageNavPosition").style.display = "";
					
				}
				setTimeout("showTab("+tabId+")", 2000);
			}
		}
	}
	setTimeout("showTab("+tabId+")", 2000);
}
