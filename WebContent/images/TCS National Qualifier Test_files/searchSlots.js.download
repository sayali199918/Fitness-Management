function fnGetDependentStates(countryId) {
	document.getElementById('District').options.length = 1;
	if (countryId == "") {
		document.getElementById('State').options.length = 1;
	} else {
		var stateCombo = document.getElementById('State');
		stateCombo.options.length = 0;
		var i = 1;
		stateCombo.options[0] = new Option("---Select---", "");
		if (states[countryId] != null && states[countryId] != "") {
			for ( var fieldCount = 0; fieldCount < states[countryId].length; fieldCount++) {
				var state = states[countryId][fieldCount];
				var stateId = state.split(",")[0];
				var stateName = state.split(",")[1];
				stateCombo.options[i] = new Option(stateName, stateId);
				i++;
			}
		}
	}
}
function fnGetDependentDistricts(stateId) {
	if (stateId == "") {
		document.getElementById('District').options.length = 1;
	} else {
		var districtCombo = document.getElementById('District');
		districtCombo.options.length = 0;
		var i = 1;
		districtCombo.options[0] = new Option("---Select---", "");
		if (districts[stateId] != null && districts[stateId] != "") {
			for ( var fieldCount = 0; fieldCount < districts[stateId].length; fieldCount++) {
				var district = districts[stateId][fieldCount];
